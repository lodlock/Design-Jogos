    <instance objName="obj_sign1" x="1709" y="560" name="inst_63AD5FDC" locked="0" code="text = &quot;Carefull with the spider. Your kid is too#afraid of enemies.Don't let them touch the kid#Otherwise you loose!&quot;;&#xA;" scaleX="1" scaleY="1" colour="4294967295" rotation="0"/>
