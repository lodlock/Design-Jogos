    <instance objName="obj_sign1" x="746" y="352" name="inst_027644CA" locked="0" code="text = &quot;Only your pet can light torches.#To controll him press F.#He can also see in the dark&quot;;&#xA;" scaleX="1" scaleY="1" colour="4294967295" rotation="0"/>
    <instance objName="obj_sign1" x="646" y="352" name="inst_1334C96C" locked="0" code="text = &quot;The kid has to stay in light#Otherwise fear will consume him#And you will loose the game!#Check your courage bar on the left upper corner#&quot;;&#xA;" scaleX="1" scaleY="1" colour="4294967295" rotation="0"/>
    <instance objName="obj_sign1" x="1041" y="353" name="inst_DC9878CB" locked="0" code="text = &quot;Use the pet now to light the torches ahead#So the kid can move on!#But don't go too far#The kid will be scared if you do!&quot;;&#xA;" scaleX="1" scaleY="1" colour="4294967295" rotation="0"/>
